   "source": [
    "def power(n):\n",
    "    if n == 1 or n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * power(n-1)\n",
    "def euler(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return 1 / power(n) + euler(n-1)\n",
    "round(euler(20),5)"
   ]
  }
